{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace",
      "Microsoft.AspNetCore.Hosting": "Trace",
      "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Trace",
      "Microsoft.AspNetCore.Server.Kestrel.Connections": "Trace",
      "Grpc": "Trace"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
